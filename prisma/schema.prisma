generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int       @id @default(autoincrement())
  name       String
  email      String    @unique
  password   String
  birthDate  String
  age        Int
  gender     String
  bio        String
  picture  String
  createdAt  DateTime  @default(now())
}

